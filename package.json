{
  "name": "brentkirklandcom",
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@fastly/js-compute": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.23.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {},
  "scripts": {
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "dev": "concurrently -c \"auto\" --kill-others \"bun run dev:auth\" \"bun run dev:www\"",
    "dev:auth": "bun run --cwd=packages/auth dev",
    "dev:www": "bun run --cwd=packages/www dev"
  }
}
